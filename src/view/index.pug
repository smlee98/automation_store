extends ../templates/base

block contents
    a.d-none(href="/src/view/board-detail.pug")
    section
        .container
            .vstack.gap-4.py-4
                h1.fw-bold Automation Store
                #top-bar.card.border-0.rounded-4
                    .card-body.p-5
                        .d-flex.flex-column.flex-lg-row.justify-content-lg-between.align-items-lg-center
                            h5.fw-bold.text-light.mb-4.mb-lg-0 함께 나눌 때 더 쉽고 빨라집니다.
                            a.btn.btn-lg.btn-primary.fw-bold(
                                href="/src/view/board-new.pug"
                            ) #[i.bi.bi-pencil] Automation Store 등록
                #search-bar.card.border-0.rounded-4
                    .card-body.p-5
                        .row.g-4
                            .col-12.col-lg-2
                                select#searchType.form-select.form-select-lg.h-100
                                    option(value="all") 전체
                                    option(value="assets") 에셋
                                    option(value="services") 서비스
                            .col-12.col-lg-10
                                .position-relative
                                    input.form-control.form-control-lg.h-100(
                                        type="text"
                                    )
                                    button.btn.btn-light.rounded-circle.position-absolute.top-50.end-0.translate-middle #[i.bi.bi-search.fs-5.text-primary]
                .d-flex.justify-content-between.align-items-center
                    legend 신규
                    .page-arrow.hstack.gap-3
                        span.arrow-before #[i.bi.bi-arrow-left-circle.fs-4]
                        span.arrow-after #[i.bi.bi-arrow-right-circle.fs-4]
                .row.row-cols-1.row-cols-md-2.row-cols-lg-4.g-4
                    .col
                        .card.card-item(
                            onclick="location.href='/board-detail.html';"
                        )
                            .card-body
                                .vstack.gap-2
                                    h5.text-primary.fw-bold 에셋
                                    .hstack.gap-2.text-muted
                                        span 기타
                                        .vr
                                        span RPA
                                    h4.fw-bold.card-title 날짜 계산기
                                    p.card-text.text-muted Javascript를 이용한 날짜 계산 Sample Task입니다. ①초라도 안보이면 ②렇게 초조한데
                                    .hstack.gap-2
                                        span.badge.rounded-pill.text-bg-light #task
                                        span.badge.rounded-pill.text-bg-light #headless
                                        span.badge.rounded-pill.text-bg-light #script
                                        span.badge.rounded-pill.text-bg-light #simple
                                hr
                                .hstack.gap-3.justify-content-end.text-muted.small
                                    .hstack.gap-1
                                        i.bi.bi-download
                                        span 20
                                    .hstack.gap-1
                                        i.bi.bi-chat-square-text
