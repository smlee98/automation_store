extends ../templates/base

block contents
    +breadcrumb(["Automation Store", "마이페이지"])
    section
        .container
            .vstack.gap-4.py-4
                h1.fw-bold 마이페이지
                #info-bar.card.rounded-4
                    .card-body.p-5
                        .vstack.gap-4
                            .d-flex.justify-content-between.align-items-center
