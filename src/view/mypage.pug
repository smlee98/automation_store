extends ../templates/base

block contents
    +breadcrumb(["Automation Store", "마이페이지"])
    section
        .container
            .vstack.gap-4.py-4
                h1.fw-bold 마이페이지
                #info-bar.card.rounded-4
                    .card-body.p-5
                        .vstack.gap-4
                            .d-flex.justify-content-between.align-items-center
                                .vstack.gap-2
                                    h4.fw-bold admin
                                    p.mb-0.text-muted 관리자
                                button.btn.btn-board.btn-outline-primary #[i.bi.bi-gear] 내 정보 설정
                            hr.my-0
                            .row.g-3
                                .col-12.col-lg-3
                                    .vstack.gap-2
                                        span.text-muted 게시글 작성
                                        h4.fw-bold.text-primary 0
                                .col-12.col-lg-3
                                    .vstack.gap-2
                                        span.text-muted 댓글 작성
                                        h4.fw-bold.text-primary 0
                                .col-12.col-lg-6
                                    .vstack.gap-2
                                        span.text-muted 자주 사용하는 해시태그
                                        .hstack.gap-2
                                            span.badge.rounded-pill.text-bg-light #task
                                            span.badge.rounded-pill.text-bg-light #headless
                                            span.badge.rounded-pill.text-bg-light #script
                                            span.badge.rounded-pill.text-bg-light #simple
                legend 내 게시글
