                                        span.badge.rounded-pill.text-bg-light #headless
                                        span.badge.rounded-pill.text-bg-light #script
                                        span.badge.rounded-pill.text-bg-light #simple
                                hr
                                .hstack.gap-3.justify-content-end.text-muted.small
                                    .hstack.gap-1
