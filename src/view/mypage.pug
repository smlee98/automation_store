                                    h4.fw-bold admin
                                    p.mb-0.text-muted 관리자
                                button.btn.btn-board.btn-outline-primary
                                    .hstack.gap-2.justify-content-center
                                        i.bi.bi-gear
                                        span 내 정보 설정
                            hr.my-0
                            .row.g-3
                                .col-12.col-lg-3
                                    .vstack.gap-2
                                        span.text-muted Public 게시글 작성
                                        h4.fw-bold.text-primary 13
                                .col-12.col-lg-3
                                    .vstack.gap-2
                                        span.text-muted Private 게시글 작성
                                        h4.fw-bold.text-primary 3
                                //- .col-12.col-lg-3
                                //-     .vstack.gap-2
